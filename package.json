{
  "name": "phytons-site",
  "version": "1.0.0",
  "description": "Site of Mikhail Kryuchkov",
  "main": "app.js",
  "scripts": {
    "start": "parcel src/index.pug",
    "build": "parcel build src/index.pug",
    "ssr-dev": "parcel watch ssr/src.js --out-file dist.js --out-dir ssr --target node",
    "build-ssr": "node ssr/dist.js"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "4.4.9",
    "redux": "4.0.0"
  },
  "author": "phytonmk",
  "license": "ISC",
  "jshintConfig": {
    "esnext": true,
    "lastsemic": true,
    "experimental": [
      "asyncawait",
      "asyncreqawait"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.3",
    "pug": "^2.0.3"
  }
}
